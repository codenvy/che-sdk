[
  {
    "id": "che-in-che",
    "creator": "ide",
    "name": "Eclipse Che",
    "description": "Utilities to build Che in Che with JDK 8 and Maven.",
    "scope": "general",
    "tags": [
      "Java",
      "JDK",
      "Maven",
      "Git"
    ],
    "components": [
      {
        "name": "JDK",
        "version": "1.8.0_92"
      },
      {
        "name": "Maven",
        "version": "3.3.9"
      }
    ],
    "source": {
      "type": "image",
      "origin": "ksmster/che-in-che"
    },
    "workspaceConfig": {
      "environments": {
        "default": {
          "machines": {
            "dev-machine": {
              "installers": [
                "org.eclipse.che.exec",
                "org.eclipse.che.terminal",
                "org.eclipse.che.ws-agent",
                "org.eclipse.che.ssh",
                "org.eclipse.che.ls.js-ts"
              ],
              "servers": {
                "GWT-CodeServer": {
                  "protocol": "http",
                  "port": "9876"
                },
                "Traefik-stat": {
                  "protocol": "http",
                  "port": "8086"
                },
                "IDE-agent-server": {
                  "protocol": "http",
                  "port": "8087"
                },
                "IDE-server": {
                  "protocol": "http",
                  "port": "8085"
                }
              },
              "attributes": {
                "memoryLimitBytes": "7516192768"
              }
            }
          },
          "recipe": {
            "location": "ksmster/che-in-che",
            "type": "dockerimage"
          }
        }
      },
      "name": "default",
      "defaultEnv": "default",
      "description": null,
      "commands": [
        {
          "commandLine": "cd ${current.project.path} && mvn clean install  -Pnative",
          "name": "Build project",
          "type": "custom",
          "attributes": {
            "goal": "Run",
            "previewUrl": ""
          }
        },
        {
          "commandLine": "cd ${current.project.path} && mvn gwt:codeserver -pl :assembly-ide-war -am -Pfast,sdm-in-che -Dskip-enforce -Dskip-validate-sources -Dche.dto.skip -Dmaven.main.skip -Dmaven.resources.skip -Dmaven.buildNumber.skip",
          "name": "GWT SDM",
          "type": "gwt_sdm_che",
          "attributes": {
            "goal": "Run",
            "previewUrl": "${server.GWT-CodeServer}"
          }
        },
        {
          "commandLine": "cd /home/user/tomcat8/bin && export SERVER_PORT=8080 && ./catalina.sh run",
          "name": "Tomcat8-IDE Start",
          "type": "custom",
          "attributes": {
            "goal": "Run",
            "previewUrl": ""
          }
        },
        {
          "commandLine": "cd /home/user/traefik && ./traefik",
          "name": "Traefik Start",
          "type": "custom",
          "attributes": {
            "goal": "Run",
            "previewUrl": ""
          }
        },
        {
          "commandLine": "cp ${current.project.path}/assembly/assembly-ide-war/target/*.war /home/user/tomcat8/webapps/IDE.war",
          "name": "Deploy IDE",
          "type": "custom",
          "attributes": {
            "goal": "Run",
            "previewUrl": "${server.IDE-server}/che/che-in-che"
          }
        },
        {
          "commandLine": "cp ${current.project.path}/assembly/assembly-wsagent-war/target/*.war /home/user/tomcat8/webapps/ROOT.war",
          "name": "Deploy Workspace Agent",
          "type": "custom",
          "attributes": {
            "goal": "Run",
            "previewUrl": "${server.IDE-server}/che/che-in-che"
          }
        }
      ]
    },
    "stackIcon": {
      "name": "type-che.svg",
      "mediaType": "image/svg+xml"
    }
  }
]
