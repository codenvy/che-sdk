workspace.runtime.image.location="codenvy/alpine_jdk8" 
workspace.commands[0].name="2. Build Che in Che"
workspace.commands[0].commandLine="mvn clean install -f /projects/che/assembly/assembly-main"

workspace.commands[1].name="3. Run Che in Che"
workspace.commands[1].commandLine='export CHE_VERSION="nightly" && export CHE_BIN_PATH=$(ls -d /projects/che/assembly/assembly-main/target/eclipse-che-*/eclipse-che-*); sudo docker run --rm -t -v /var/run/docker.sock:/var/run/docker.sock --env CHE_LOCAL_BINARY=${CHE_BIN_PATH/\'/projects/che\'/$(sudo docker inspect --format \'{{ range .Mounts }}{{ if eq .Destination "/projects/che" }}{{ .Source }}{{ end }}{{ end }}\' $(hostname))}  --env CHE_PORT=54321 --env CHE_SERVER_CONTAINER_NAME="che-in-che-server" codenvy/che-launcher:nightly start'
workspace.commands[1].attributes={
"previewUrl": "http://localhost:54321"
}
workspace.commands[2].name="4. Stop Che in Che"
workspace.commands[2].commandLine='export CHE_VERSION="nightly" && sudo docker run --rm -t -v /var/run/docker.sock:/var/run/docker.sock --env CHE_SERVER_CONTAINER_NAME="che-in-che-server" codenvy/che-launcher stop'

workspace.commands[3].name="4. Stop Che in Che"
workspace.commands[3].commandLine='sudo docker run --rm -t -v /var/run/docker.sock:/var/run/docker.sock --env CHE_SERVER_CONTAINER_NAME="che-in-che-server" codenvy/che-launcher stop'

workspace.commands[4].name="** Kill Che in Che **"
workspace.commands[4].commandLine="sudo docker rm -f che-in-che-server"

workspace.name="che"
workspace.ram=3092
